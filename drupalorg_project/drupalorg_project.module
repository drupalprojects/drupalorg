/**
 * Count number of issues in various issue queues on drupal.org.
 *
 * This function only counts issues for core releases that we're currently
 * tracking.
 *
 * @see drupalorg_project_bingo_block_output()
 * @see DRUPALORG_CORE_VERSION_TID
 * @see DRUPALORG_CORE_VERSION_NAME
 */
function drupalorg_project_issue_counts() {
  // @todo D7 replace {project_issues} with EntityFieldQuery.
  // Fetch issue counts for the current development release, and a combined
  // count for the development release and the latest stable release.
  $versions = array(DRUPALORG_CORE_DEVELOPMENT_VERSION_TID => array(DRUPALORG_CORE_DEVELOPMENT_VERSION_TID), 'both' => array(DRUPALORG_CORE_DEVELOPMENT_VERSION_TID, DRUPALORG_CORE_STABLE_VERSION_TID));

  foreach ($versions as $version => $value) {
    $issue_rids = db_query("SELECT prn.nid FROM {project_release_nodes} prn INNER JOIN {node} n ON prn.nid = n.nid AND n.status = 1 WHERE prn.pid = :pid AND prn.version_api_tid IN (:versions)", array(':pid' => DRUPALORG_CORE_NID, ':versions' => $value))->fetchCol();
    $args = array(':pid' => DRUPALORG_CORE_NID, ':issue_rids' => $issue_rids);
    $issue_counts[$version]['Critical bugs'] = db_query("SELECT COUNT(*) FROM {project_issues} pi INNER JOIN {node} n ON pi.nid = n.nid WHERE n.status = 1 AND pid = :pid AND category = 'bug' AND sid IN (1,8,13,14,15) AND priority = 1 AND rid IN (:issue_rids)", $args)->fetchField();
    $issue_counts[$version]['Major bugs'] = db_query("SELECT COUNT(*) FROM {project_issues} pi INNER JOIN {node} n ON pi.nid = n.nid WHERE n.status = 1 AND pid = :pid AND category = 'bug' AND sid IN (1,8,13,14,15) AND priority = 4 AND rid IN (:issue_rids)", $args)->fetchField();
    $issue_counts[$version]['Critical tasks'] = db_query("SELECT COUNT(*) FROM {project_issues} pi INNER JOIN {node} n ON pi.nid = n.nid WHERE n.status = 1 AND pid = :pid AND category = 'task' AND sid IN (1,8,13,14,15) AND priority = 1 AND rid IN (:issue_rids)", $args)->fetchField();
    $issue_counts[$version]['Major tasks'] = db_query("SELECT COUNT(*) FROM {project_issues} pi INNER JOIN {node} n ON pi.nid = n.nid WHERE n.status = 1 AND pid = :pid AND category = 'task' AND sid IN (1,8,13,14,15) AND priority = 4 AND rid IN (:issue_rids)", $args)->fetchField();
    $issue_counts[$version]['Patch queue'] = db_query("SELECT COUNT(*) FROM {project_issues} pi INNER JOIN {node} n ON pi.nid = n.nid WHERE n.status = 1 AND pid = :pid AND sid IN (8,13,14) AND rid IN (:issue_rids)", $args)->fetchField();
    $issue_counts[$version]['Pending bugs'] = db_query("SELECT COUNT(*) FROM {project_issues} pi INNER JOIN {node} n ON pi.nid = n.nid WHERE n.status = 1 AND pid = :pid AND category = 'bug' AND sid IN(1,8,13,14,15) AND rid IN (:issue_rids)", $args)->fetchField();
  }
  $issue_counts['Novice issues'] = db_query("SELECT COUNT(*) FROM {project_issues} pi INNER JOIN {term_node} tn ON pi.nid = tn.nid INNER JOIN {term_data} td on tn.tid = td.tid WHERE td.name = 'Novice' AND pi.sid IN (1, 8, 13, 14, 15)")->fetchField();
  variable_set('drupalorg_project_issue_counts', $issue_counts);
}

